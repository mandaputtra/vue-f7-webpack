  "license": "UNLICENSED",
  "framework7": {
    "cwd": "D:\\Latihan\\vuef7",
    "type": [
      "cordova",
      "web",
      "pwa"
    ],
    "name": "vuef7",
    "pkg": "com.mandaputtra.vuef7",
    "platform": [
      "ios",
      "android"
    ],
    "framework": "vue",
    "template": "split-view",
    "cssPreProcessor": "stylus",
    "customColor": false,
    "bundler": "webpack"
  },
  "scripts": {
    "build-prod": "cross-env NODE_ENV=production node ./build/build.js",
    "build-cordova-prod": "cross-env TARGET=cordova cross-env NODE_ENV=production node ./build/build.js && cd cordova && cordova build",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.js",
    "start": "npm run dev",
    "lint:fix": "xo ./src --fix --ext .js,.vue --format",
    "lint": "xo ./src --ext .js,.vue --format"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix",
      "pre-push": "npm lint:fix"
    }
  },
  "xo": {
    "semicolon": false,
    "envs": ["browser"]
  },
  "browserslist": [
    "Android >= 5",
    "IOS >= 9.3",
    "Edge >= 15",
    "Safari >= 9.1",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
  "dependencies": {
    "dom7": "^2.1.3",
    "framework7": "^4.2.0",
    "framework7-icons": "^2.3.0",
